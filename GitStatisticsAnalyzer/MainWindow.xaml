<Window x:Class="GitStatisticsAnalyzer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GitStatisticsAnalyzer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" FontFamily="Arial" FontSize="13.333">
    <Grid>
        <TextBlock x:Name="versionTextBlock" HorizontalAlignment="Right" Margin="0,35,10,0" TextWrapping="Wrap" Text="Git-Version" VerticalAlignment="Top" TextAlignment="Right" Width="118"/>
        <StatusBar Height="20" Margin="0" VerticalAlignment="Bottom" Width="517" FontFamily="Arial" FontSize="13.333">
            <TextBlock FontSize="13.333" FontFamily="Arial" HorizontalAlignment="Right"/>
        </StatusBar>
        <Menu x:Name="menu" Height="30" VerticalAlignment="Top" Width="517">
            <MenuItem/>
        </Menu>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="121,169,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
