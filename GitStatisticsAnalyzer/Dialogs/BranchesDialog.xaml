<Window x:Name="window" x:Class="GitStatisticsAnalyzer.Dialogs.BranchesDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GitStatisticsAnalyzer.Dialogs"
        mc:Ignorable="d"
        Title="Enter Branches" Height="138" Width="335" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="OkButtonClick"/>
        <TextBox x:Name="firstBranchTextBox" Height="23" Margin="104,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="{Binding Path=FirstBranch, ElementName=window}" TabIndex="1" RenderTransformOrigin="0.167,-0.957"/>
        <TextBox x:Name="secondBranchTextBox" Height="23" Margin="104,38,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="{Binding Path=SecondBranch, ElementName=window}" TabIndex="2"/>
        <Label Content="First Branch" HorizontalAlignment="Left" Margin="10,9,0,0" VerticalAlignment="Top"/>
        <Label Content="Second Branch" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
